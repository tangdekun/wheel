<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tdk.baseres">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".UtilsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/baseres_ic_launcher"
        android:label="@string/baseres_app_name"
        android:roundIcon="@mipmap/baseres_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/baseres_AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestActivity"></activity>
    </application>

</manifest>